<configuration>
	<generalSettings>
		<azureSubscriptionName>{subscription-name}</azureSubscriptionName>
		<location>{location}</location>
		<adminUserName>{admin-user}</adminUserName>
		<adminPassword>{admin-password}</adminPassword>
		<storageAccount>{storageAccount}</storageAccount>
	</generalSettings>
	<puppetMasterSettings>	
		<cloudServiceName>{master-cloudServiceName}</cloudServiceName>
		<vmName>{master-vmName}</vmName>
		<consoleUsername>{console-user}</consoleUsername>
		<consolePassword>{console-password}</consolePassword>
	</puppetMasterSettings>
	<puppetAgentSettings>
		<cloudServiceName>{agent-cloudServiceName}</cloudServiceName>
		<vmName>{agent-vmName}</vmName>
	</puppetAgentSettings>
</configuration>